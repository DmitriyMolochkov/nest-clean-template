{
  "name": "nest-clean-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "prepare": "husky install",
    "start": "nest start --exec \"node --env-file .env\"",
    "start:dev": "nest start --exec \"node --env-file .env\" --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint -c .eslintrc.json .",
    "lint:fix": "npm run lint -- --fix",
    "test-spec": "jest --config jest.spec.config.ts",
    "typeorm": "node --env-file .env -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- -d ./db/dataSource.ts",
    "migration:generate": "cross-env-shell \"npm run typeorm -- -d ./db/dataSource.ts migration:generate ./db/migrations/$npm_config_name\"",
    "migration:create": "cross-env-shell \"npm run typeorm -- migration:create ./db/migrations/$npm_config_name\"",
    "migration:revert": "npm run typeorm -- -d ./db/dataSource.ts migration:revert",
    "migration:show": "npm run typeorm -- migration:show -d ./db/dataSource.ts"
  },
  "dependencies": {
    "@bull-board/api": "^5.9.1",
    "@bull-board/express": "^5.9.1",
    "@bull-board/nestjs": "^5.9.1",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "bullmq": "^4.12.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "ioredis": "^5.3.2",
    "ldapjs": "^3.0.5",
    "nestjs-pino": "^3.5.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pino": "^8.16.1",
    "pino-http": "^8.5.0",
    "pino-pretty": "^10.2.3",
    "postgres": "^3.3.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.9",
    "@types/jest": "^29.5.2",
    "@types/ldapjs": "^3.0.3",
    "@types/node": "^20.8.9",
    "@types/passport-local": "^1.0.37",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-import-newlines": "^1.3.4",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^15.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
